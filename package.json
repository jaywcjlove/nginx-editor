{
  "name": "monaco-editor-nginx",
  "version": "1.0.10",
  "description": "Nginx language for Monaco Editor.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "homepage": "https://jaywcjlove.github.io/nginx-editor",
  "scripts": {
    "prepare": "npm run build",
    "doc": "GENERATE_SOURCEMAP=false kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "watch": "tsbb watch",
    "build": "tsbb build",
    "get:nginx": "ts-node --project scripts/tsconfig.json scripts/directives.ts",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/nginx-editor.git"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@babel/runtime": ">=7.10.0",
    "monaco-editor": ">=0.22.3",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "keywords": [
    "monaco-editor",
    "monaco-nginx",
    "nginx",
    "monaco",
    "editor",
    "editor-nginx",
    "monaco-editor-nginx"
  ],
  "devDependencies": {
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@uiw/react-monacoeditor": "3.4.4",
    "@types/turndown": "5.0.1",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "cheerio": "1.0.0-rc.10",
    "compile-less-cli": "1.8.8",
    "domhandler": "4.2.2",
    "node-fetch": "2.6.2",
    "husky": "7.0.2",
    "kkt": "6.11.0",
    "lint-staged": "11.1.2",
    "monaco-editor-webpack-plugin": "4.2.0",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "turndown": "7.1.1",
    "ts-node": "10.2.1",
    "tsbb": "3.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
